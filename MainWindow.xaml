<Window x:Class="_184905IPO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_184905IPO"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <StackPanel>
        <Label>Width</Label>
        <TextBox x:Name="txtWidth" />
        <Label>Length</Label>
        <TextBox x:Name="txtLength" />
        <Button x:Name="btnCalculate" Content="Calculate Area" Click="btnCalculate_Click" />
        <Label x:Name="lblOutput" Content="" />
    </StackPanel>
</Window>
